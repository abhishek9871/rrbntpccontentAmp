<style type="text/css">
    .chat-container {
        height: 450px;
        overflow-y: scroll;
        padding: 0px 5px;
    }

    .chat-chatbot .card-body,
    .chat-human .card-body,
    .chat-chatbot .card-header,
    .chat-human .card-header {
        padding: 5px 10px;
    }

    #jumping-dots-container {
        width: -webkit-fill-available;
    }

    .jumping-dots span {
        position: relative;
        margin-left: auto;
        margin-right: auto;
        animation: jump 1s infinite;
        display: inline-block;
        background-color: #0D7751;
        width: 12px;
        height: 12px;
        border-radius: 50%;
        margin-right: 3px;
    }

    .jumping-dots .dot-1 {
        animation-delay: 200ms;
    }

    .jumping-dots .dot-2 {
        animation-delay: 400ms;
    }

    .jumping-dots .dot-3 {
        animation-delay: 600ms;
    }

    @keyframes jump {
        0% {
            bottom: 0px;
        }

        20% {
            bottom: 5px;
        }

        40% {
            bottom: 0px;
        }
    }

    .chat-container {
        display: flex;
        flex-direction: column;
        gap: 10px;
        padding: 20px;
        /* max-width: 500px; */
    }

    /* Common chat bubble style */
    .chat-bubble {
        max-width: 70%;
        padding: 12px 16px;
        border-radius: 16px;
        position: relative;
        font-family: Arial, sans-serif;
        font-size: 14px;
        line-height: 1.4;
    }

    /* Left-aligned bubble */
    .chat-bubble.left {
        background-color: #f1f0f0;
        color: #000;
        align-self: flex-start;
        border-bottom-left-radius: 0;
    }

    .chat-bubble.left::after {
        content: "";
        position: absolute;
        left: -8px;
        bottom: 0;
        width: 0;
        height: 0;
        border: 10px solid transparent;
        border-top-color: #f1f0f0;
        border-bottom: 0;
        border-right: 0;
        margin-left: -1px;
        margin-bottom: -10px;
    }

    /* Right-aligned bubble */
    .chat-bubble.right {
        background-color: #0084ff;
        color: #fff;
        align-self: flex-end;
        border-bottom-right-radius: 0;
    }

    .chat-bubble.right::after {
        content: "";
        position: absolute;
        right: -8px;
        bottom: 0;
        width: 0;
        height: 0;
        border: 10px solid transparent;
        border-top-color: #0084ff;
        border-bottom: 0;
        border-left: 0;
        margin-right: -1px;
        margin-bottom: -10px;
    }
</style>
<script type="text/javascript">
    $(document).ready(function() {
        $("#chat-text").on('keypress', function(event) {
            if (event.key === 'Enter') {
                event.preventDefault()
                send_chat();
            }
        });
    });

    function send_chat() {
        var query = $("#chat-text").val();
        var obj = {
            'query': query
        };

        if ($("#chat-text").val()) {
            $("#chat-msgs").append('<div class="chat-bubble right">\
                                        <p class="text-muted small mb-0"><i class="far fa-clock"></i> ' + new Date().toLocaleTimeString() + '</p>\
                                        <p>' + query + '</p>\
                                    </div>\
                                    ');
        }
        $("#chat-msgs").append('<div class="float-start" id="jumping-dots-container">\
                                    <i class="fa-solid fa-robot text-success"></i> <small class="text-muted">Chatbot is processing your request...</small>\
                                    <div class="jumping-dots">\
                                        <span class="dot-1"></span>\
                                        <span class="dot-2"></span>\
                                        <span class="dot-3"></span>\
                                    </div>\
                                </div>');
        $("#chat-msgs").scrollTop($("#chat-msgs").height()).animate("fast");
        $("#chat-text").val("");

        $.ajax({
            url: "/ajax/chatbot.php",
            method: "POST",
            timeout: 0,
            contentType: "application/json",
            data: JSON.stringify(obj),
            success: function(response, textStatus, jQxhr) {
                $("#chat-msgs").append('<div class="chat-bubble left">\
                                            <p class="text-muted small mb-0"><i class="far fa-clock"></i> ' + new Date().toLocaleTimeString() + '</p>\
                                                ' + response.reply + '\
                                            </p>\
                                        </div>');
            },
            error: function(jqXhr, textStatus, errorThrown) {
                $("#modal_server_error").modal("show");
                console.log(errorThrown);
            },
            complete: function(jqXHR, textStatus) {
                startTypeSetting("result");
                $("#jumping-dots-container").remove();
            }
        });
    }
</script>

<div class="modal fade" id="modal_chatbot" tabindex="-1" aria-hidden="true" data-bs-backdrop="static" data-bs-keyboard="false">
    <div class="modal-dialog modal-xl" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">
                    <i class="fa-solid fa-comments"></i> I will try my best to help you...
                </h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true"></span>
                </button>
            </div>
            <div class="modal-body">
                <div class="chat-container" id="chat-msgs"></div>
                <div id="chat-input" class="mt-4">
                    <div class="input-group">
                        <input type="text" class="form-control" aria-describedby="button-chatbot-send" placeholder="&nbsp;Enter your desired topic here" id="chat-text">
                        <button class="btn btn-primary" type="button" id="button-chatbot-send" onclick="javascript:send_chat()"><i class="fa-solid fa-paper-plane"></i> Send</button>
                    </div>
                    <div class="badge-group" style="display: none;"></div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">
                    <i class="fa-solid fa-times"></i> Close
                </button>
            </div>
        </div>
    </div>
</div>
