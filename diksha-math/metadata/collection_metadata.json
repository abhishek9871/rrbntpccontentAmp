				$(".sidebar").height(height);
				if (window.innerWidth < 800) {
					$("#sitch-ctrl").removeClass("open");
					$("#sitch-ctrl").addClass("close");
					$("#sitch-ctrl").html('<i class="fa-solid fa-angles-left"></i>');
					$("#stitching-pane").hide();
					$("#content-pane").removeClass("col-8");
					$("#content-pane").addClass("col-12");
				} else {
					$("#sitch-ctrl").removeClass("close");
					$("#sitch-ctrl").addClass("open");
					$("#sitch-ctrl").html('<i class="fa-solid fa-angles-right"></i>');
					$("#content-pane").removeClass("col-12");
					$("#content-pane").addClass("col-8");
					$("#stitching-pane").show();
				}
			}).trigger("resize");

			// stitching open/close
			$("#sitch-ctrl").on("click", function() {
				if ($("#sitch-ctrl").hasClass("open")) {
					$("#sitch-ctrl").removeClass("open");
					$("#sitch-ctrl").addClass("close");
					$("#sitch-ctrl").html('<i class="fa-solid fa-angles-left"></i>');
					$("#stitching-pane").hide();
					$("#content-pane").removeClass("col-8");
					$("#content-pane").addClass("col-12");
				} else {
					$("#sitch-ctrl").removeClass("close");
					$("#sitch-ctrl").addClass("open");
					$("#sitch-ctrl").html('<i class="fa-solid fa-angles-right"></i>');
					$("#content-pane").removeClass("col-12");
					$("#content-pane").addClass("col-8");
					$("#stitching-pane").show();
				}
			});

