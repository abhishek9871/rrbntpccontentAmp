<script>
    var pages = 0;
    var lockpage = 0;

    function loadmore() {
        if (lockpage == 0) {
            pages++;
            addData();
        }
    }

    var loaderdiv = '<div class="col-lg-12 removelistitems" id="postget_2695">\
                    <div class="card cus-shadow mt-4 mb-2">\
                    <div class="card-body p-0 clearfix">\
                    <div class="file-left text-center">\
                    <div class="sketchdiv imgsmsk"></div></div>\
                    <div class="file-right pr-3">\
                        <span class="sketchdiv mt-3 mb-2 text-left"></span>\
                        <span class="sketchdiv d-inline-block text-left widsk20"></span>\
                        <span class="sketchdiv d-inline-block text-left widsk20"></span>\
                        <span class="sketchdiv d-inline-block text-left widsk20"></span>\
                        <span class="sketchdiv text-left widsk90"></span>\
                    </div></div></div></div>';

    function beforeload() {
        for (var i = 0; i < 10; i++) {
            $('#get_response').append(loaderdiv);
        }
    }
    $(window).scroll(function() {
        if ($(window).scrollTop() == $(document).height() - $(window).height()) {
            loadmore();
        }
    });

    function addData() {
        beforeload();
        $.ajax({
            method: "POST",
            url: "https://pdf.exampundit.in/post-list",
            data: {
                page: pages,
                                                paid: '0'
                            },
            success: function(res) {
                if (res == '') {
                    lockpage = 1;
                }
                $('.removelistitems').remove();
                $('#get_response').append(res);
            }
        });
    }
    $(document).ready(function() {
        addData();
    });

    function saveDrive(id) {
        $.ajax({
            url: 'https://pdf.exampundit.in/load-section',
            data: {
                saveid: id,
                remove: '0'
            },
            success: function(d) {
                if (d == '1') {
                    $('#postremovebtn').show();
                    $('#postsavebtn').hide();
                    swal({
                        title: "Success !!",
                        text: "Successfully added on your drive !!",
                        type: "success",
                        timer: 1000,
                        showConfirmButton: false
                    });
                } else {
                    swal({
                        title: "Login Please !!",
                        text: "Login and try again..",
                        type: "error",
                        timer: 1000,
                        showConfirmButton: false
                    });
                }
            }
        });
    }

    function removeDrive(id) {
        swal({
            title: "Are you sure to Removed ?",
            text: "You will be able to recover to save again !!",
            type: "warning",
            showCancelButton: !0,
            confirmButtonColor: "#DD6B55",
            confirmButtonText: "Yes, Removed it !!",
            closeOnConfirm: !1,
            showLoaderOnConfirm: true
        }, function() {
            $.ajax({
                url: 'https://pdf.exampundit.in/load-section',
                data: {
                    saveid: id,
                    remove: '1'
                },
                success: function(d) {
                    if (d == '1') {
                        swal({
                            title: "Removed !!",
                            text: "Hey, your saved PDF has been Removed !!",
                            type: "success",
                            timer: 1000,
                            showConfirmButton: false
                        });
                        $('#postremovebtn').hide();
                        $('#postsavebtn').show();
                    } else {
                        swal({
                            title: "Please try again !!",
                            text: "Something went wrong try again..",
                            type: "error",
                            timer: 1000,
                            showConfirmButton: false
                        });
                    }
                }
            });
        });
    }


    function buynowget(id, linkcode) {
        window.location.href = "https://pdf.exampundit.in/pay/pdf/" + linkcode;
    }
    
    
    
    function savecount(id) {
        console.log('Login and try again..');
    }
    
</script>
